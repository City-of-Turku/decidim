<div class="row">
  <div class="columns">
    <% if highlighted? || voted? %>
      <div class="section">
        <%= render :highlighted %>
        <%= render :voted %>
      </div>
    <% end %>

    <% if voting_finished? %>
      <%= render :card_list %>
    <% else %>
      <%= render :turku_list %>
    <% end %>
  </div>
</div>
